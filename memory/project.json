{
  "name": "context-aware-mcp",
  "purpose": "A context-aware memory system for AI coding assistants that provides persistent memory, project structure mapping, and integration with tools like Cursor, GitHub Copilot, and Claude.",
  "languages": ["JavaScript", "Node.js"],
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.13.0",
    "fs-extra": "^11.1.1",
    "dotenv": "^16.3.1"
  },
  "key_frameworks": ["Express.js", "WebSocket"],
  "memory_system": {
    "file_types": ["project.json", "architecture.md", "decisions.md", "changelog.md", "todos.md", "structure.mmd", "structure.json", "development-guidelines.md"],
    "slash_commands": ["/remember", "/done", "/map", "/decision"]
  },
  "integrations": ["Cursor", "GitHub Copilot", "Claude"]
} 